[mjs-desktop]
image=mjs-desktop:latest
{{- if eq .chezmoi.hostname "terra"}}
nvidia=true
{{ else }}
nvidia=false
{{ end -}}
replace=true
start_now=true
exported_apps="keepassxc emacs nvim librewolf"
exported_bins="/usr/bin/keepassxc /usr/bin/keepassxc-cli /usr/bin/librewolf /usr/bin/emacs /usr/bin/nvim /usr/bin/eza /usr/bin/htop /usr/bin/btop /usr/bin/rg /usr/bin/wlogout /usr/bin/i3-wk-switch"
